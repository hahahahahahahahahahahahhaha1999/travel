<%= turbo_frame_tag dom_id(article, :like) do %>
  <% if user_signed_in? && article.liked_by?(current_user)%>
    <%=link_to "いいね済", article_likes_path(article.id), data: { turbo_method: :delete }, class: "tweet_likes" %>
  <% else %>
    <%=link_to "いいねする", article_likes_path(article.id), data: { turbo_method: :post }, class: "tweet_likes" %>
  <% end %>
<% end %>